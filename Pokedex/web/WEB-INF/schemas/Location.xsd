<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="https://www.pokedex.com/schemas/Location.xsd"
            xmlns="https://www.pokedex.com/schemas/Location.xsd"
            elementFormDefault="qualified">  
    <xsd:complexType name="Location">
        <xsd:sequence>
            <xsd:element name="id" type="xsd:positiveInteger"/>
            <xsd:element name="location" type="xsd:string"/>          
            <xsd:element name="regionId" type="xsd:positiveInteger"/>                 
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
