<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="https://www.pokedex.com/schemas/PokemonGeneration.xsd"
            xmlns="https://www.pokedex.com/schemas/PokemonGeneration.xsd"
            elementFormDefault="qualified">  
    <xsd:complexType name="PokemonGeneration">
        <xsd:sequence>
            <xsd:element name="pokemonId" type="xsd:positiveInteger"/>
            <xsd:element name="appearInGame" type="xsd:string"/>          
            <xsd:element name="localIndexNo" type="xsd:positiveInteger"/>              
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
