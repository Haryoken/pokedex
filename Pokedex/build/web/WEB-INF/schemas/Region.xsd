<xsd:schema version="1.0"
            xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="https://www.pokedex.com/schemas/Region.xsd"
            xmlns="https://www.pokedex.com/schemas/Region.xsd"
            elementFormDefault="qualified">  
    <xsd:element name="Region">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="id" type="xsd:positiveInteger"/>
                <xsd:element name="regionName" type="xsd:string"/>          
                <xsd:element name="mapURI" type="xsd:anyURI"/>                 
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>
</xsd:schema>
